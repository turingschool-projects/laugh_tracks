<h1>Welcome to the Center of the Comedian Universe</h1>
<br>
<img src="https://media0.giphy.com/media/YkXNjAkG7CfEVx3gcy/giphy.gif" alt="Spinning Globe">
<br>
<ul>
  <li><a href="/comedians">Comedian Index</a></li>
  <li><a href="/comedians/age?=:Age">Comedian Age Check</a></li>
  <li><a href="/comedians/new">New Comedian</a></li>
</ul>




<% @comedians.each do |comedian| %>
<section id="comedian-<%=comedian.id%>">
  <br>
    <img src=  <%= comedian.image_url %> alt= <%= comedian.name.delete(" ") %> >
    <h2>Comedian name: <%= comedian.name %></h2>
    <p>Age: <%= comedian.age %></p>
    <p>Birthplace: <%= comedian.birthplace %></p>
    <p>Total Specials: <%= comedian.specials.length %></p>

    <% comedian.specials.each do |special|  %>

    <p>Special name: <%= special.name %></p>
    <p>Special runtime <%= special.runtime_mins %></p>
    <% end %>
  <% end %>
</section>
